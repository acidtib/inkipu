<div class="row">
  <div class="col-12">
    <h2>Players</h2>

    <table class="table">
      <thead>
        <tr>
          <th>BattleNet</th>
          <th>Wins</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @players.each do |player| %>
          <tr>
            <td><%= player.bnet_name %></td>
            <td><%= player.wins %></td>
            <td><%= link_to 'Show', player %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>