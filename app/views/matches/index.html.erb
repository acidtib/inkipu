<div class="row">
  <div class="col-12">
    <h2>Matches</h2>

    <table class="table">
      <thead>
        <tr>
          <th>Bnet match</th>
          <th>Player</th>
          <th>Opponent</th>
          <th>Winner</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @matches.each do |match| %>
          <tr>
            <td><%= match.bnet_match_id %></td>
            <td><%= match.player.bnet_name %></td>
            <td><%= match.opponent.bnet_name %></td>
            <td><%= match.winner %></td>
            <td><%= link_to 'Show', match %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>